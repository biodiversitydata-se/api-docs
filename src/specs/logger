        required: true
      responses:
        "200":
          description: Create a new Event log
components:
  schemas:
    LogEventVO:
      type: object
      properties:
        comment:
          type: string
        eventTypeId:
          type: integer
          format: int32
        userIP:
          type: string
        userAgent:
          type: string
        recordCounts:
          type: object
          additionalProperties:
            type: integer
            format: int32
        userEmail:
          type: string
        month:
          type: string
        reasonTypeId:
          type: integer
          format: int32
        sourceTypeId:
          type: integer
          format: int32
        sourceUrl:
          type: string
        recordCount:
          type: object
          additionalProperties:
            type: integer
            format: int32
          writeOnly: true
        recordCountConcurrent:
          type: object
          additionalProperties:
            type: object
            properties:
              opaque:
                type: integer
                format: int32
              acquire:
                type: integer
                format: int32
              release:
                type: integer
                format: int32
                writeOnly: true
              andIncrement:
                type: integer
                format: int32
              andDecrement:
                type: integer
                format: int32
              plain:
                type: integer
                format: int32
          writeOnly: true
